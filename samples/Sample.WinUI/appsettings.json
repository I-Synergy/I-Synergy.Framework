{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "OpenTelemetry": {
      "LogLevel": {
        "Default": "Trace"
      },
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "Features": {
    "CheckForUpdatesInMicrosoftStore": false
  },
  "SentryOptions": {
    "Dsn": "https://123456",
    "Environment": "Development",
    "SampleRate": 0.1,
    "TracesSampleRate": 0.1,
    "ProfilesSampleRate": 0.2,
    "SendDefaultPii": true,
    "Debug": true
  },
  "ConfigurationOptions": {
    "ClientId": "client_id",
    "ClientSecret": "client_secret"
  },
  "NugetOptions": {
    "ApiKey": "12345"
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:18889"
}