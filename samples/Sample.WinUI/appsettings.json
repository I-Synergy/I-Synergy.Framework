{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Features": {
    "CheckForUpdatesInMicrosoftStore": false
  },
  "SentryOptions": {
    "Dsn": "https://123456",
    "StackTraceMode": "Enhanced",
    "DiagnosticLevel": "info",
    "MaxBreadcrumbs": 50,
    "Environment": "Development",
    "SampleRate": 1.0,
    "TracesSampleRate": 0.1,
    "ProfilesSampleRate": 0.2,
    "SendDefaultPii": true,
    "CaptureFailedRequests": true,
    "Debug": true
  },
  "ConfigurationOptions": {
    "ClientId": "client_id",
    "ClientSecret": "client_secret"
  },
  "NugetOptions": {
    "ApiKey": "12345"
  },
  "OpenTelemetryOptions": {
    "OTLPExporterEndpoint": "http://localhost:18889"
  }
}