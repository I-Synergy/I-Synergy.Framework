  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "filter": "filter.yml",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "I-Synergy Framework Documentation",
      "_appFooter": "",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/discordfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
