<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\Analyzers.props" />
  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <RootNamespace>ISynergy.Framework</RootNamespace>
    <LangVersion>7.3</LangVersion>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <AssemblyVersion>2019.0.0.0</AssemblyVersion>
    <FileVersion>2019.0.0.0</FileVersion>
    <Authors>Ismail Hassani</Authors>
    <Company>International Intelligence</Company>
    <Product>I-Synergy Corporate Business Management</Product>
