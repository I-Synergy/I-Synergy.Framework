<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\project.props" />

    <PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
        <PackageId>I-Synergy.Framework.AspNetCore.Authentication</PackageId>
        <Description>I-Synergy Framework Authentication for .net 8.0</Description>
        <PackageTags>I-Synergy, Framework, AspNetCore, Authentication, OAuth</PackageTags>
		<GenerateLibraryLayout>true</GenerateLibraryLayout>
		<CodeAnalysisRuleSet>ISynergy.Framework.AspNetCore.Authentication.ruleset</CodeAnalysisRuleSet>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="7.0.3" />
    </ItemGroup>

	<ItemGroup>
        <ProjectReference Include="..\ISynergy.Framework.AspNetCore\ISynergy.Framework.AspNetCore.csproj" />
    </ItemGroup>
</Project>
