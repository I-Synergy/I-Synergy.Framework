<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\Project.props" />
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>ISynergy.Framework.AspNetCore.WebDav.Server</RootNamespace>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\output\I-Synergy.Framework.AspNetCore.WebDav.Server\debug</OutputPath>
    <DocumentationFile>..\..\output\I-Synergy.Framework.AspNetCore.WebDav.Server\debug\I-Synergy.Framework.AspNetCore.WebDav.Server.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\..\output\I-Synergy.Framework.AspNetCore.WebDav.Server\release</OutputPath>
    <DocumentationFile>..\..\output\I-Synergy.Framework.AspNetCore.WebDav.Server\release\I-Synergy.Framework.AspNetCore.WebDav.Server.xml</DocumentationFile>
    <NoWarn>1701;1702</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Utils\UAParser\UAParser.regexes.yaml" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Utils\UAParser\UAParser.regexes.yaml" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.0" />
    <PackageReference Include="System.Collections.Immutable" Version="1.7.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
