<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\..\project.props" />
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackageId>I-Synergy.Framework.Mail.Microsoft365</PackageId>
		<Description>I-Synergy Framework Mail with Microsoft 365 for .net Standard 2.0</Description>
		<PackageTags>I-Synergy, Framework, Mail, Microsoft, Graph, Office, 365</PackageTags>
		<GenerateLibraryLayout>true</GenerateLibraryLayout>
		<RootNamespace>ISynergy.Framework.Mail</RootNamespace>
		<CodeAnalysisRuleSet>ISynergy.Framework.Mail.Microsoft365.ruleset</CodeAnalysisRuleSet>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Azure.Identity" Version="1.9.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="7.0.1" />
		<PackageReference Include="Microsoft.Graph" Version="5.15.0" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\ISynergy.Framework.Mail\ISynergy.Framework.Mail.csproj" />
	</ItemGroup>

	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>$(MSBuildProjectName).Tests,</_Parameter1>
		</AssemblyAttribute>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>DynamicProxyGenAssembly2,</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>

	<ItemGroup>
	  <AdditionalFiles Include="..\..\.sonarlint\i-synergy.framework\CSharp\SonarLint.xml" Link="SonarLint.xml" />
	</ItemGroup>
</Project>
