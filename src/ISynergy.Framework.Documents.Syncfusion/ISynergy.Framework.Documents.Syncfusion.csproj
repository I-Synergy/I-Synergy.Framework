<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\project.props" />
    <PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
		<PackageId>I-Synergy.Framework.Documents.Syncfusion</PackageId>
		<Description>I-Synergy Framework Documents with Syncfusion for .Net Standard 2.0 and .Net 6.0</Description>
		<PackageTags>I-Synergy, Framework, Documents, Syncfusion</PackageTags>
		<GenerateLibraryLayout>true</GenerateLibraryLayout>
		<IncludeSymbols>true</IncludeSymbols>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSymbols>true</IncludeSymbols>
		<IncludeBuildOutput>true</IncludeBuildOutput>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\ISynergy.Framework.Core\ISynergy.Framework.Core.csproj" />
        <ProjectReference Include="..\ISynergy.Framework.Documents\ISynergy.Framework.Documents.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Syncfusion.DocIO.Net.Core" Version="19.3.0.53" />
        <PackageReference Include="Syncfusion.DocIORenderer.Net.Core" Version="19.3.0.53" />
        <PackageReference Include="Syncfusion.Licensing" Version="19.3.0.53" />
        <PackageReference Include="Syncfusion.Pdf.Net.Core" Version="19.3.0.53" />
        <PackageReference Include="Syncfusion.XlsIO.Net.Core" Version="19.3.0.53" />
    </ItemGroup>

</Project>
