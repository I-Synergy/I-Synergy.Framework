<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\project.props" />
    <PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<PackageId>I-Synergy.Framework.Documents.Syncfusion</PackageId>
		<Description>I-Synergy Framework Documents with Syncfusion for .Net 6.0</Description>
		<PackageTags>I-Synergy, Framework, Documents, Syncfusion</PackageTags>
		<GenerateLibraryLayout>true</GenerateLibraryLayout>
		<IncludeSymbols>true</IncludeSymbols>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSymbols>true</IncludeSymbols>
		<IncludeBuildOutput>true</IncludeBuildOutput>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<RootNamespace>ISynergy.Framework.Documents</RootNamespace>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\ISynergy.Framework.Core\ISynergy.Framework.Core.csproj" />
        <ProjectReference Include="..\ISynergy.Framework.Documents\ISynergy.Framework.Documents.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Syncfusion.DocIO.Net.Core" Version="19.3.0.54" />
        <PackageReference Include="Syncfusion.DocIORenderer.Net.Core" Version="19.3.0.54" />
        <PackageReference Include="Syncfusion.Licensing" Version="19.3.0.54" />
        <PackageReference Include="Syncfusion.Pdf.Net.Core" Version="19.3.0.54" />
        <PackageReference Include="Syncfusion.XlsIO.Net.Core" Version="19.3.0.54" />
    </ItemGroup>

	<ItemGroup>
		<Using Include="ISynergy.Framework.Core.Abstractions.Services" />
		<Using Include="ISynergy.Framework.Core.Extensions" />
		<Using Include="ISynergy.Framework.Core.Validation" />
		<Using Include="ISynergy.Framework.Documents.Abstractions.Services" />
		<Using Include="ISynergy.Framework.Documents.Models" />
		<Using Include="Syncfusion.DocIO.DLS" />
		<Using Include="Syncfusion.DocIORenderer" />
		<Using Include="Syncfusion.XlsIO" />
		<Using Include="System.Collections" />
	</ItemGroup>
</Project>
