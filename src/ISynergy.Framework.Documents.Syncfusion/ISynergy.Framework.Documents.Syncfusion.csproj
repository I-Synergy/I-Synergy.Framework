<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\..\project.props" />
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
		<PackageId>I-Synergy.Framework.Documents.Syncfusion</PackageId>
		<Description>I-Synergy Framework Documents with Syncfusion for .net Standard 2.0, .net 6.0, .net 7.0 and .net 8.0</Description>
		<PackageTags>I-Synergy, Framework, Documents, Syncfusion</PackageTags>
		<GenerateLibraryLayout>true</GenerateLibraryLayout>
		<RootNamespace>ISynergy.Framework.Documents</RootNamespace>
		<CodeAnalysisRuleSet>ISynergy.Framework.Documents.Syncfusion.ruleset</CodeAnalysisRuleSet>
	</PropertyGroup>
	<ItemGroup>
		<ProjectReference Include="..\ISynergy.Framework.Core\ISynergy.Framework.Core.csproj" />
		<ProjectReference Include="..\ISynergy.Framework.Documents\ISynergy.Framework.Documents.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Syncfusion.DocIO.Net.Core" Version="24.1.43" />
		<PackageReference Include="Syncfusion.DocIORenderer.Net.Core" Version="24.1.43" />
		<PackageReference Include="Syncfusion.Licensing" Version="24.1.43" />
		<PackageReference Include="Syncfusion.Pdf.Net.Core" Version="24.1.43" />
		<PackageReference Include="Syncfusion.XlsIO.Net.Core" Version="24.1.43" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="8.0.0" />
	</ItemGroup>
</Project>
