<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\project.props" />
    <PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
		<PackageId>I-Synergy.Framework.Printer.Label.Dymo</PackageId>
		<Description>I-Synergy Framework Dymo Label printer library for .net Standard 2.0, .net 6.0, .net 7.0 and .net 8.0</Description>
		<PackageTags>I-Synergy, Framework, Printer, Label, Dymo</PackageTags>
		<GenerateLibraryLayout>true</GenerateLibraryLayout>
		<CodeAnalysisRuleSet>ISynergy.Framework.Printer.Label.Dymo.ruleset</CodeAnalysisRuleSet>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="DYMO.Connect.SDK" Version="1.4.3.37" />
    </ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)'!='net8.0'">
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)'=='net8.0'">
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
	</ItemGroup>
	
    <ItemGroup>
        <ProjectReference Include="..\ISynergy.Framework.Printer.Label\ISynergy.Framework.Printer.Label.csproj" />
    </ItemGroup>
</Project>
